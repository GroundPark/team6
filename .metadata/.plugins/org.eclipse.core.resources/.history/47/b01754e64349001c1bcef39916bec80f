package product.vo;

public class ProductInfo {
	private String pi_id, pi_name, pi_img1, pi_img2, pi_img3, pi_desc, pi_soldout, pi_isview, pi_date;
	private int pi_idx, pc_id, pi_price, ai_idx, pi_salecnt, pi_good, pi_stock, pi_review;
	private double pi_discount, pi_score;
	
	// 조인해서 가져올 데이터
	private String pc_name;

	public String getPi_id() {
		return pi_id;
	}

	public void setPi_id(String pi_id) {
		this.pi_id = pi_id;
	}

	public String getPi_name() {
		return pi_name;
	}

	public void setPi_name(String pi_name) {
		this.pi_name = pi_name;
	}

	public String getPi_img1() {
		return pi_img1;
	}

	public void setPi_img1(String pi_img1) {
		this.pi_img1 = pi_img1;
	}

	public String getPi_img2() {
		return pi_img2;
	}

	public void setPi_img2(String pi_img2) {
		this.pi_img2 = pi_img2;
	}

	public String getPi_img3() {
		return pi_img3;
	}

	public void setPi_img3(String pi_img3) {
		this.pi_img3 = pi_img3;
	}

	public String getPi_desc() {
		return pi_desc;
	}

	public void setPi_desc(String pi_desc) {
		this.pi_desc = pi_desc;
	}

	public int getPi_idx() {
		return pi_idx;
	}

	public void setPi_idx(int pi_idx) {
		this.pi_idx = pi_idx;
	}

	public int getPc_id() {
		return pc_id;
	}

	public void setPc_id(int pc_id) {
		this.pc_id = pc_id;
	}

	public int getPi_price() {
		return pi_price;
	}

	public void setPi_price(int pi_price) {
		this.pi_price = pi_price;
	}

	public int getAi_idx() {
		return ai_idx;
	}

	public void setAi_idx(int ai_idx) {
		this.ai_idx = ai_idx;
	}

	public int getPi_score() {
		return pi_score;
	}

	public void setPi_score(int pi_score) {
		this.pi_score = pi_score;
	}

	public int getPi_salecnt() {
		return pi_salecnt;
	}

	public void setPi_salecnt(int pi_salecnt) {
		this.pi_salecnt = pi_salecnt;
	}

	public int getPi_good() {
		return pi_good;
	}

	public void setPi_good(int pi_good) {
		this.pi_good = pi_good;
	}

	public int getPi_stock() {
		return pi_stock;
	}

	public void setPi_stock(int pi_stock) {
		this.pi_stock = pi_stock;
	}

	public int getPi_review() {
		return pi_review;
	}

	public void setPi_review(int pi_review) {
		this.pi_review = pi_review;
	}

	public double getPi_discount() {
		return pi_discount;
	}

	public void setPi_discount(double pi_discount) {
		this.pi_discount = pi_discount;
	}

	public double getPi_soldout() {
		return pi_soldout;
	}

	public void setPi_soldout(double pi_soldout) {
		this.pi_soldout = pi_soldout;
	}

	public double getPi_isview() {
		return pi_isview;
	}

	public void setPi_isview(double pi_isview) {
		this.pi_isview = pi_isview;
	}

	public double getPi_date() {
		return pi_date;
	}

	public void setPi_date(double pi_date) {
		this.pi_date = pi_date;
	}

	public String getPc_name() {
		return pc_name;
	}

	public void setPc_name(String pc_name) {
		this.pc_name = pc_name;
	}
	
	
}
